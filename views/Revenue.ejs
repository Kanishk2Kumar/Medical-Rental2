<body>
    <div class="graphbox">
        <h2>Revenue Genrated</h2>
        <div class="box">
            <canvas id="myChart"></canvas>
        </div>
    </div>

    <script>
       const ctx = document.getElementById('myChart');

new Chart(ctx, {
  type: 'bar',
  data: {
    labels: [
                'Mobility Aids',
                'Respiratory Equipment',
                'Patient Monitoring',
                'Rehabilitation Equipment',
                'Hospital Beds',
                'Daily Living Aids',
                'Incontinence Products',
                'Wound Care',
                'Diagnostics Equipment',
                'Emergency Equipment'
            ],
    datasets: [{
      label: 'Sales',
      data: [12, 19, 3, 5, 2, 3,10,9,5,5],
      borderWidth: 1
    }]
  },
  options: {
    scales: {
      y: {
        beginAtZero: true
      }
    }
  }
});
    </script>
</body>
