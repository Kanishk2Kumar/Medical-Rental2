<div class="AddProducts_body">
    <h1 class="aProductss">Add Medicine</h1>
    <form id="MedicineForm" action="/AddMedicine" method="post" enctype="multipart/form-data" class="AddProducts_form">
      <label for="title" class="AddProducts_label">Title</label>
      <input type="text" id="title" name="title" placeholder="Enter medicine title" class="AddProducts_input" required />
  
      <label for="description" class="AddProducts_label">Description</label>
      <textarea id="description" name="description" rows="5" placeholder="Enter description" class="AddProducts_textarea" required></textarea>
  
      <label for="price" class="AddProducts_label">Price</label>
      <input type="number" id="price" name="price" placeholder="Enter price" class="AddProducts_input" required />
  
      <label for="categories" class="AddProducts_label">Categories</label>
      <input type="text" id="categories" name="categories" placeholder="Enter categories (comma separated)" class="AddProducts_input" />
  
      <label for="image" class="AddProducts_label">Image</label>
      <input type="file" id="image" name="image" class="AddProducts_input" required />
  
      <section class="AddProducts_createbutton">
        <button type="submit">Add Medicine</button>
      </section>
    </form>
  </div>
  